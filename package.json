{
  "name": "@labelinsight/redux-module",
  "version": "0.0.4",
  "description": "The ultimate Redux boilerplate reducer",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && babel src/*.mjs --ignore \"**/*.spec.mjs\" --out-dir dist --source-maps",
    "clean": "rm -rf dist && mkdir dist",
    "lint": "eslint . --ext js,mjs",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest src/**/*.spec.js",
    "test:dev": "npm t -- --watch"
  },
  "keywords": [
    "redux"
  ],
  "author": "Joshua Kleckner <github.com/lokua> (jkleckner@labelinsight.com)",
  "repository": {
    "type": "git",
    "url": "git://github.com/labelinsight/redux-module.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/eslint-parser": "7.13.10",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-env": "7.8.4",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "25.1.0",
    "prettier": "2.2.1"
  }
}
